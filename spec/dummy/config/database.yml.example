# Fill this out if you want to use the Marty::SqlServer specs
SQLSERVER_<%= Rails.env %>:
  sqlserver: true
  host:         #
  database:     #
  username:     #
  password:     #
  test_command: #

common: &common
  adapter: postgresql

development:
  <<: *common
  url: <%= ENV['DEVELOPMENT_DATABASE_URL'] %>
  database: marty_dev

test:
  <<: *common
  database: marty_test
  url: <%= ENV['TEST_DATABASE_URL'] %>
